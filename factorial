import java.util.Scanner;
class swap{
        public static void main(String[] args)
        {
            Scanner sc=new Scanner(System.in);
            int x=sc.nextInt();
            int n=1;
           for(int i=x;i>0;i--)
           {
               n=n*i;
           }
            System.out.println("factorial of "+x+" is "+n);
        }
}
