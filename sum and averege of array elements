import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int sum=0;
       
        int[] nums=new int[n];
         for(int i=0;i<n;i++)
        {
            nums[i]=sc.nextInt();
            sum=sum+nums[i];
        }
            
        System.out.println("sum of elements="+sum);
        float avrg=0;
        avrg=sum/n;
        System.out.println("avrg of elements="+avrg);
        
    }
}
