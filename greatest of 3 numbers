import java.util.Scanner;
class swap{
        public static void main(String[] args)
        {
            Scanner sc=new Scanner(System.in);
            int x=sc.nextInt();
            int y=sc.nextInt();
            int z=sc.nextInt();
            if(x>y)
            {
                if(x>z)
                {
                    System.out.println(x+" is greatest");
                }
                else
                {
                    System.out.println(z+" is greatest");
                }
                
            }else
            {
               System.out.println(y+" is greatest"); 
            }
        }
}
