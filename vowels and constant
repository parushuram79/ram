import java.util.Scanner;
class main{
        public static void main(String[] args)
        {
            Scanner sc=new Scanner(System.in);
            int ch=sc.next().charAt(0);
            if (ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u')
            {
                System.out.println("Vowels");
            }
            else
            {
              System.out.println("Constant");  
            }
        }
}
