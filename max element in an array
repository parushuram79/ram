import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
       
        int[] nums=new int[n];
         int max=nums[0];
        for(int i=0;i<n;i++)
        {
            nums[i]=sc.nextInt();
            if(nums[i]>max)
        {
            max=nums[i];
        }
        }
        System.out.println("Max ele="+max);
        
    }
}
