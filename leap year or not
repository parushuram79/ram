import java.util.Scanner;
class swap{
        public static void main(String[] args)
        {
            Scanner sc=new Scanner(System.in);
            int x=sc.nextInt();
            
            if(x%4==0)
            {
               System.out.println(x+" is leap year");
        }else
        {
            System.out.println(x+" is  not a leap year");
        }
        }
}
