import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
       
        int[] nums=new int[n];
         nums[0]=sc.nextInt();
         int min=nums[0];
        
        for(int i=1;i<n;i++)
        {
            nums[i]=sc.nextInt();
            if(nums[i]<min)
        {
            min=nums[i];
        }
        }
        System.out.println("min ele="+min);
        
    }
}
